{
	"spec_dir": "src",
	"spec_files": [
		"**/*.spec.ts"
	],
	"helpers": [
		"../scripts/jasmine-console-helpers.js"
	],
	"reporters": [
		{
			"name": "jasmine-spec-reporter#SpecReporter",
			"options": {
				"displayStacktrace": "all"
			}
		}
	],
	"stopSpecOnExpectationFailure": false,
	"captureExceptions": true,
	"random": false
}
